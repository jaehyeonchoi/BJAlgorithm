#include <iostream>
#include <string>

using namespace std;

int main() {
	int total;
	int n;
	int price, count;

	cin >> total;
	cin >> n;

	for (int i = 0; i < n; i++) {
		cin >> price >> count;
		total -= price * count;
	}

	if (total == 0) {
		cout << "Yes";
	}
	else {
		cout << "No";
	}

	return 0;
}
